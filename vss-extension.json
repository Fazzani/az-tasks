{
  "manifestVersion": 1,
  "id": "sonar-project-build-release-task",
  "name": "SonarCloud project manager Build and Release Tools",
  "version": "0.0.1",
  "publisher": "synker",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tools for building/releasing with sonarcloud. Includes one build/release task.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "tasks/sonar"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/sonar"
      }
    }
  ]
}
